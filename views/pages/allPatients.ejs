<%- contentFor('css') %>
<link rel="stylesheet" href="/css/signin.css">


<%- contentFor('body') %>

<div>
  <table id="table" >
          <thead>
              <tr>
                  <th>Health Care Number</th>
                  <th>First Name</th>
                  <th>Middle Name</th>
                  <th>Last Name</th>
                  <th>Sex</th>
                  <th>Email</th>
              </tr>
          </thead>
           <tbody>
           <% results.forEach(function (patient) { %>
              <tr>
                  <td><%= patient.healthcarenum %></td>
                  <td><%= patient.fname %></td>
                  <td><%= patient.mname %></td>
                  <td><%= patient.lname %></td>
                  <td><%= patient.sex %></td>
                  <td><%= patient.email %></td>
              </tr>
           <% }) %>
           </tbody>
      </table>
</div>
